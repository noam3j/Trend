<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewTrendDay">
        <attr name="day" format="integer" />
    </declare-styleable>

    <declare-styleable name="ViewTrendDayOfWeek">
        <attr name="dayName" format="integer" />
    </declare-styleable>

    <declare-styleable name="ViewTrendMonthName">
        <attr name="month" format="integer" />
        <attr name="year" format="integer" />
    </declare-styleable>

    <declare-styleable name="ImageViewSVG">
        <attr name="file" format="string" />
    </declare-styleable>

    <declare-styleable name="ImageButtonSVG">
        <attr name="file_idle" format="string" />
        <attr name="file_pressed" format="string" />
    </declare-styleable>
    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

</resources>